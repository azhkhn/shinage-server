{
  "name": "mztx/shinage-server",
  "license": "MIT",
  "type": "project",
  "description": "Digital signage server component based on Symfony.",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "mztx\\ShinageOnlinePlayerBundle\\": "_old_src/ShinageOnlinePlayerBundle/",
      "shinage\\server\\behat\\": "features/bootstrap/"
    },
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "spec\\App\\": "spec/",
      "DoctrineMigrations\\": "src/Migrations/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",

    "doctrine/orm": "^2.7",
    "doctrine/doctrine-bundle": "^2.1",
    "friendsofsymfony/user-bundle": "dev-master",
    "studio-42/elfinder": "^2.1",
    "guzzlehttp/guzzle": "^6.5",
    "knplabs/knp-menu-bundle": "^3.0",
    "jms/serializer-bundle": "^3.7",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "mschop/pathogen": "^0.6.1",
    "symfony/dependency-injection": "5.4.*",
    "symfony/config": "5.4.*",
    "symfony/flex": "^2.0",
    "symfony/twig-bundle": "5.4.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/swiftmailer-bundle": "^3.5",
    "symfony/form": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/templating": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/asset": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "sensio/framework-extra-bundle": "^5.2",
    "scheb/2fa-bundle": "^5.0",
    "scheb/2fa-email": "^5.0",
    "scheb/2fa-google-authenticator": "^5.0",
    "scheb/2fa-backup-code": "^5.0",
    "endroid/qr-code-bundle": "^3.4.3",
    "symfony/cache": "5.4.*",
    "psr/simple-cache": "^1.0",
    "composer/package-versions-deprecated": "1.11.99.4",
    "symfony/proxy-manager-bridge": "5.4.*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "5.4.*",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "phpspec/phpspec": "^6.2",
    "behat/behat": "^3.7",
    "friends-of-behat/mink-extension": "^2.3",
    "behat/mink-goutte-driver": "^2.0",
    "friends-of-behat/symfony-extension": "^2.0",
    "symplify/easy-coding-standard": "^10.0",
    "cweagans/composer-patches": "~1.0",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "php install-git-hooks.php"
    ],
    "post-update-cmd": [
      "php install-git-hooks.php"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "platform": {
      "php": "8.0.14"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": true,
      "cweagans/composer-patches": true,
      "symfony/flex": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "5.4.*"
    },
    "patches": {
    },
    "composer-exit-on-patch-failure": true
  }
}
